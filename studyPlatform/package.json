{
  "name": "studyplatform",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "concurrently \"node app.js\" \"npm run dev\"",
    "test": "npm run test:database && npm run test:selenium",
    "test:database": "mocha 'tests/database_tests/**/*.js' --timeout 5000",
    "test:selenium": "mocha 'tests/selenium_tests/**/*.js' --timeout 10000"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "chai": "^5.1.2",
    "concurrently": "^9.0.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "mocha": "^10.7.0",
    "mongo": "^0.1.0",
    "mongodb": "^6.6.1",
    "mongoose": "^8.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.28.0",
    "selenium-webdriver": "^4.23.0",
    "vite-express": "^0.16.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "mongodb-memory-server": "^10.1.3",
    "vite": "^5.2.0"
  }
